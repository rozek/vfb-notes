{
  "name": "vfb-notes",
  "version": "0.1.0",
  "description": "a simple demonstrator for voltcloud-for-browsers",
  "type": "module",
  "scripts": {
    "build": "rimraf ./dist && mkdir ./dist && cp ./src/index.html ./dist && rollup -c",
    "dev": "rimraf ./test && mkdir ./test && cp ./src/index.html ./test && rollup -c -w",
    "start": "sirv ./test --port 7654 --host --cors",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rozek/vfb-notes.git"
  },
  "keywords": [
    "voltcloud",
    "web-application"
  ],
  "author": "Andreas Rozek <a.rozek@gmx.de> (https://www.rozek.de/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rozek/vfb-notes/issues"
  },
  "homepage": "https://github.com/rozek/vfb-notes#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-typescript": "^8.2.5",
    "@tsconfig/svelte": "^2.0.1",
    "jszip": "^3.7.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.42.1",
    "svelte-preprocess": "^4.7.4",
    "terser": "^5.7.1",
    "typescript": "^4.3.5",
    "voltcloud-for-nodejs": "^0.1.3"
  },
  "dependencies": {
    "@stablelib/base64": "^1.0.1",
    "javascript-interface-library": "^1.0.0",
    "svelte-viewport-info": "^0.1.12",
    "tweetnacl": "^1.0.3",
    "voltcloud-for-browsers": "^0.1.3"
  }
}
